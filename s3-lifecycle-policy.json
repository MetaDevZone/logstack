{
  "Rules": [
    {
      "ID": "LogstackRetentionPolicy",
      "Status": "Enabled",
      "Filter": {
        "Prefix": "production-logs"
      },
      "Expiration": {
        "Days": 180
      },
      "NoncurrentVersionExpiration": {
        "NoncurrentDays": 30
      },
      "AbortIncompleteMultipartUpload": {
        "DaysAfterInitiation": 7
      }
    }
  ]
}
